notepad index.html
<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>Basic HTML Page</title>
</head>
<body>
    <header>
        <h1>Welcome to My Web Page</h1>
    </header>

    <section>
        <h2>About Me</h2>
        <p>This is a basic HTML page example.</p>
    </section>

    <footer>
        <p>&copy; 2024 My Web Page</p>
    </footer>
</body>
</html>
touch Dockerfile
notepad Dockerfile
# Use the official Nginx image from Docker Hub
FROM nginx:alpine

# Copy the index.html file into the Nginx default public directory
COPY index.html /usr/share/nginx/html/

# Expose port 80 to allow external access to the website
EXPOSE 80

# Run Nginx in the foreground (default command for the Nginx container)
CMD ["nginx", "-g", "daemon off;"]
docker build -t monday




